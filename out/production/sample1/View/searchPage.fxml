         </font>
      </JFXButton>
      <JFXButton fx:id="unmute" layoutX="611.0" layoutY="375.0" onAction="#unmute" prefHeight="94.0" prefWidth="107.0" style="-fx-background-radius: 50; -fx-border-radius: 50;" text="unmute" visible="false">
         <font>
            <Font name="Segoe UI Light" size="21.0" />
         </font>
      </JFXButton>
      <ImageView fitHeight="31.0" fitWidth="42.0" layoutX="45.0" layoutY="55.0" onMouseClicked="#returnToTimeLine" opacity="0.48" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Icons/backarrow.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="direct" layoutX="408.0" layoutY="344.0" onAction="#direct" prefHeight="94.0" prefWidth="86.0" style="-fx-background-radius: 50; -fx-border-radius: 50;" text="direct" visible="false">
