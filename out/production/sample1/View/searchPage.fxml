<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.searchPage_Controller">
   <children>
      <Label layoutX="273.0" layoutY="67.0" prefHeight="69.0" prefWidth="512.0" text="Search for users to find your friends !">
